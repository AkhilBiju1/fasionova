<section>

    <center>
        <div class="container">
            <div class="col-lg-12">
                <div class="card mb-4">
                    <div class="card-body">
                        {{#each user}}
                        <div class="row">
                            <div class="col-sm-3">
                                <p class="mb-0">Full Name</p>
                            </div>
                            <div class="col-sm-9">
                                <p class="text-muted mb-0">{{this.name}} </p>
                            </div>
                        </div>
                        <hr>
                        {{/each}}
                       
                    </div>
                </div>
            </div>
    </center>
</section>